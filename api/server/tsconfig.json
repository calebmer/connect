{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": true,
    "lib": ["es2017"],
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}
