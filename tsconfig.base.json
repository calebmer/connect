{
  "compilerOptions": {
    "noEmit": true,
    "lib": ["es2017"],
    "allowJs": true,
    "isolatedModules": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}
